{
    "name": "python-import-helper",
    "displayName": "Bar Nuri Tools Python Import Helper",
    "description": "Automatically generate import statements for Python.",
    "publisher": "Bar",
    "version": "1.0.0",
    "scripts": {
        "build": "tsc",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "gitBeforePublish": "npm version patch && git add . && git commit -m \"version\" --allow-empty && git push",
        "publish": "npm run gitBeforePublish && vsce publish",
        "removeUnusedImports": "tslint --config tslint-imports.json --fix --project ."
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Formatters",
        "Other"
    ],
    "keywords": [
        "multi-root ready",
        "import",
        "export",
        "javascript",
        "python"
    ],
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#01263F",
        "theme": "dark"
    },
    "activationEvents": [
        "workspaceContains:**/*.py"
    ],
    "engines": {
        "vscode": "^1.31.0"
    },
    "main": "./out/extension.js",
    "dependencies": {
        "anymatch": "3.1.1",
        "fs-extra": "8.1.0",
        "lodash": "4.17.15",
        "make-dir": "3.0.0",
        "tslib": "^2.0.1",
        "utlz": "2.2.0",
        "vscode": "1.1.36"
    },
    "devDependencies": {
        "@types/make-dir": "2.1.0",
        "prettier": "1.19.1",
        "@types/anymatch": "1.3.1",
        "@types/fs-extra": "8.0.1",
        "@types/lodash": "4.14.148",
        "@types/node": "12.12.8",
        "ts-node": "8.5.2",
        "typescript": "3.7.2"
    }
}
