{
    "name": "python-import-helper",
    "displayName": "Bar Nuri Tools Python Import Helper",
    "description": "Automatically generate import statements for Python.",
    "publisher": "Bar",
    "version": "1.0.8",
    "scripts": {
        "build": "tsc -b",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "gitBeforePublish": "npm version patch && git add . && git commit -m \"version\" --allow-empty && git push",
        "publish": "npm run gitBeforePublish && vsce publish",
        "removeUnusedImports": "tslint --config tslint-imports.json --fix --project ."
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Formatters",
        "Other"
    ],
    "keywords": [
        "multi-root ready",
        "import",
        "export",
        "javascript",
        "python"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/barnuri/vscode-extensions"
    },
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#01263F",
        "theme": "dark"
    },
    "activationEvents": [
        "workspaceContains:**/*.py"
    ],
    "engines": {
        "vscode": "^1.33.0"
    },
    "main": "./out/extension.js",
    "dependencies": {
        "anymatch": "3.1.1",
        "fs-extra": "9.0.1",
        "lodash": "4.17.20",
        "make-dir": "3.1.0",
        "tslib": "^2.0.1",
        "utlz": "3.3.0"
    },
    "devDependencies": {
        "@types/make-dir": "2.1.0",
        "prettier": "2.0.5",
        "@types/anymatch": "1.3.1",
        "@types/fs-extra": "9.0.1",
        "@types/lodash": "4.14.159",
        "@types/node": "14.6.0",
        "ts-node": "8.10.2",
        "typescript": "3.9.7",
        "vscode": "1.1.37"
    }
}
