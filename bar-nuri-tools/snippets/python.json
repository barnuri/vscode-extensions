{
    "pylintrc": {
        "prefix": [".pylintrc", "pylint", "pylintrc"],
        "body": [
            "[MASTER] ",
            "ignore=\"venv,CVS\" ",
            "ignore-patterns=\"^.*\\.(?!py\\$)[^.]+\\$\" ",
            " ",
            "[MESSAGES CONTROL]  ",
            "disable=C,R,broad-except,  ",
            "        redefined-builtin,  ",
            "        missing-docstring,  ",
            "        invalid-name,  ",
            "        deprecated-lambda,  ",
            "        relative-import,  ",
            "        global-statement,  ",
            "        redefined-outer-name,  ",
            "        bare-except,  ",
            "        pointless-string-statement,  ",
            "        dangerous-default-value,  ",
            "        unresolved-import,  ",
            "        arguments-differ,  ",
            "        keyword-arg-before-vararg,  ",
            "        protected-access,  ",
            "        anomalous-backslash-in-string,  ",
            "        expression-not-assigned,  ",
            "        no-init, ",
            "        cell-var-from-loop ",
            "  ",
            "[REPORTS]  ",
            "msg-template=\"{path}:{line}: [{msg_id}({symbol}), {obj} {msg}\"  ",
            "output-format=colorized  ",
            ""
        ],
        "description": "pylintrc"
    },
    "pyproject": {
        "prefix": ["pyproject", "pyproject.toml"],
        "body": [
            "[tool.black]  ",
            "line-length = 120  ",
            "target-version = ['py36', 'py37', 'py38']  ",
            "include = '\\.pyi?\\$'  ",
            "exclude = '''  ",
            "/(  ",
            "  venv   ",
            "  | dist  ",
            "  | build ",
            ")/  ",
            "'''  ",
            ""
        ],
        "description": "pyproject.toml"
    }
}
