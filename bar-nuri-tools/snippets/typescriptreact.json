{
    "React Hooks Typescript By Filename": {
        "prefix": "reacthooks",
        "body": [
            "import * as React from 'react'; ",
            "",
            "const $TM_FILENAME_BASE: React.FC<any> = ({}) => {",
            "    return <></>; ",
            "}",
            "",
            "export default $TM_FILENAME_BASE;"
        ],
        "description": "React Hooks Typescript"
    },
    "React Hooks Typescript By Foldername": {
        "prefix": "reacthooks2",
        "body": [
            "import * as React from 'react'; ",
            "",
            "const ${TM_DIRECTORY/^.+\\\\(.*)$/$1/}: React.FC<any> = ({}) => {",
            "    return <></>; ",
            "}",
            "",
            "export default ${TM_DIRECTORY/^.+\\\\(.*)$/$1/};"
        ],
        "description": "React Hooks Typescript"
    }
}
