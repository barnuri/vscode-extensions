{
    "nodemon config": {
        "prefix": "nodemonconf",
        "body": [
            "{",
            "    \"watch\": [\"src\"],",
            "    \"ext\": \"ts\",",
            "    \"ignore\": [\"src/**/*.spec.ts\"],",
            "    \"exec\": \"tsc && node --inspect=0.0.0.0 dist/main.js\"",
            "}",
            ""
        ],
        "description": "nodemon config"
    },
    "pritter setting": {
        "prefix": "prittersetting",
        "body": [
            "{",
            "    \"bracketSpacing\": true,",
            "    \"jsxSingleQuote\": true,",
            "    \"tabWidth\": 4,",
            "    \"arrowParens\": \"avoid\",",
            "    \"endOfLine\": \"auto\",",
            "    \"htmlWhitespaceSensitivity\": \"css\",",
            "    \"jsxBracketSameLine\": false,",
            "    \"proseWrap\": \"preserve\",",
            "    \"semi\": true,",
            "    \"singleQuote\": true,",
            "    \"printWidth\": 160,",
            "    \"useTabs\": false,",
            "    \"trailingComma\": \"all\"",
            "}",
            ""
        ],
        "description": "pritter setting"
    },

    "pritter script": {
        "prefix": "pritterscript",
        "body": ["\"format\": \"prettier --write \\\"src/**/*.{js,jsx,ts,tsx}\\\"\""],
        "description": "pritter script"
    },
    "Vscode Settings": {
        "prefix": ["vscodesettings", "settings"],
        "body": [
            "{",
            "    \"files.autoSave\": \"afterDelay\",",
            "    \"editor.formatOnSave\": true,",
            "    \"workbench.startupEditor\": \"none\",",
            "    \"files.watcherExclude\": {",
            "        \"*/.git/objects/*\": true,",
            "        \"*/.git/subtree-cache/*\": true,",
            "        \"*/node_modules//**\": true,",
            "        \"editor.fontFamily\": \"Droid Sans Mono, Droid Sans Fallback\"",
            "    },",
            "    \"files.associations\": {",
            "        \"Dockerfile-*\": \"dockerfile\",",
            "        \"dockerfile-*\": \"dockerfile\",",
            "        \"jenkins*\": \"jenkinsfile\",",
            "        \"Jenkins*\": \"jenkinsfile\"",
            "    },",
            "    \"typescript.preferences.importModuleSpecifier\": \"relative\",",
            "    \"git.enableSmartCommit\": true,",
            "    \"javascript.updateImportsOnFileMove.enabled\": \"never\",",
            "    \"explorer.confirmDragAndDrop\": false,",
            "    \"explorer.confirmDelete\": false,",
            "    \"typescript.updateImportsOnFileMove.enabled\": \"always\",",
            "    \"tslint.autoFixOnSave\": true,",
            "    \"git.confirmSync\": false,",
            "    \"git.autofetch\": true,",
            "    \"git.fetchOnPull\": true,",
            "    \"editor.tabSize\": 4,",
            "    \"editor.insertSpaces\": true,",
            "    \"editor.detectIndentation\": false,",
            "    \"html.format.wrapLineLength\": 150,",
            "    \"html.format.wrapAttributes\": \"aligned-multiple\",",
            "    \"html.format.preserveNewLines\": true,",
            "    \"html.format.maxPreserveNewLines\": null,",
            "    \"html.format.endWithNewline\": true,",
            "    \"files.insertFinalNewline\": true,",
            "    \"prettier.bracketSpacing\": true,",
            "    \"prettier.jsxSingleQuote\": true,",
            "    \"prettier.tabWidth\": 4,",
            "    \"git.pullTags\": false,",
            "    \"projectManager.git.baseFolders\": [\"C:\\\\Projects\", \"D:\\\\Projects\"],",
            "    \"prettier.disableLanguages\": [\"sql\", \"SQL\", \"Dockerfile\", \"dockerfile\"],",
            "    \"[sql]\": {",
            "        \"editor.formatOnSave\": false",
            "    },",
            "    \"[dockerfile]\": {",
            "        \"editor.formatOnSave\": false",
            "    },",
            "    \"workbench.activityBar.visible\": true,",
            "    \"python.insidersChannel\": \"off\",",
            "    \"editor.codeActionsOnSave\": {",
            "        \"source.fixAll.eslint\": true",
            "    },",
            "    \"diffEditor.ignoreTrimWhitespace\": false,",
            "    \"prettier.arrowParens\": \"avoid\",",
            "    \"prettier.endOfLine\": \"auto\",",
            "    \"prettier.htmlWhitespaceSensitivity\": \"css\",",
            "    \"prettier.jsxBracketSameLine\": false,",
            "    \"prettier.proseWrap\": \"preserve\",",
            "    \"prettier.semi\": true,",
            "    \"prettier.singleQuote\": true,",
            "    \"prettier.printWidth\": 150,",
            "    \"prettier.useTabs\": false,",
            "    \"prettier.trailingComma\": \"all\",",
            "    \"todohighlight.isEnable\": true,",
            "    \"todohighlight.keywords\": [",
            "        {",
            "            \"text\": \"TODO\",",
            "            \"color\": \"white\",",
            "            \"backgroundColor\": \"green\"",
            "        }",
            "    ],",
            "    \"window.zoomLevel\": 0,",
            "    \"editor.minimap.enabled\": true,",
            "    \"breadcrumbs.enabled\": true,",
            "    \"workbench.iconTheme\": \"vscode-icons\",",
            "    \"files.eol\": \"auto\",",
            "    \"files.defaultLanguage\": \"js\",",
            "    \"files.encoding\": \"utf8\",",
            "    \"vsicons.projectDetection.disableDetect\": true,",
            "    \"terminal.integrated.rendererType\": \"dom\",",
            "    \"extensions.ignoreRecommendations\": true,",
            "    \"[python]\": {",
            "        \"editor.insertSpaces\": true,",
            "        \"editor.tabSize\": 4",
            "    },",
            "    \"eslint.migration.2_x\": \"off\",",
            "    \"explorer.compactFolders\": false,",
            "    \"remote.SSH.remotePlatform\": {",
            "        \"10.0.100.117\": \"linux\"",
            "    },",
            "    \"python.formatting.autopep8Path\": \"\",",
            "    \"python.formatting.provider\": \"none\",",
            "    \"editor.suggestSelection\": \"first\",",
            "    \"vsintellicode.modify.editor.suggestSelection\": \"automaticallyOverrodeDefaultValue\",",
            "    \"python.languageServer\": \"Microsoft\",",
            "    \"python.linting.pylintEnabled\": true,",
            "    \"python.linting.enabled\": true,",
            "    \"python.linting.lintOnSave\": true,",
            "    \"python.linting.pylintArgs\": [",
            "        \"--rcfile=.pylintrc\",",
            "        \"--disable=C,R,keyword-arg-before-vararg,protected-access,broad-except,redefined-builtin,missing-docstring,invalid-name,deprecated-lambda,relative-import,global-statement,redefined-outer-name,bare-except,pointless-string-statement,dangerous-default-value,unresolved-import,arguments-differ\",",
            "        \"--msg-template=\\\"{path}:{line}: [{msg_id}({symbol}), {obj} {msg}\\\"\",",
            "        \"--output-format=colorized\"",
            "    ]",
            "}",
            ""
        ],
        "description": "Vscode Settings"
    },
    "tsconfig": {
        "prefix": "tsconfig",
        "body": [
            "{ ",
            "    \"compilerOptions\": { ",
            "        \"module\": \"commonjs\", ",
            "        \"target\": \"esnext\", ",
            "        \"moduleResolution\": \"node\", ",
            "        \"types\": [\"reflect-metadata\"], ",
            "        \"emitDecoratorMetadata\": true, ",
            "        \"experimentalDecorators\": true, ",
            "        \"sourceMap\": true, ",
            "        \"outDir\": \"./dist\", ",
            "        \"declaration\": true, ",
            "        \"declarationMap\": true, ",
            "        \"noImplicitAny\": false, ",
            "        \"removeComments\": true, ",
            "        \"noLib\": false, ",
            "        \"allowSyntheticDefaultImports\": true, ",
            "        \"esModuleInterop\": true, ",
            "        \"importHelpers\": true, ",
            "        \"downlevelIteration\": true, ",
            "        \"strict\": true, ",
            "        \"strictPropertyInitialization\": false, ",
            "        \"alwaysStrict\": true, ",
            "        \"charset\": \"utf8\", ",
            "        \"newLine\": \"LF\", ",
            "        \"noFallthroughCasesInSwitch\": true, ",
            "        \"noImplicitReturns\": true, ",
            "        \"noUnusedLocals\": true, ",
            "        \"pretty\": true, ",
            "        \"resolveJsonModule\": true, ",
            "        \"strictBindCallApply\": true, ",
            "        \"strictNullChecks\": true, ",
            "        \"skipLibCheck\": true ",
            "    }, ",
            "    \"include\": [\"src/**/*\"], ",
            "    \"exclude\": [\"node_modules\"] ",
            "} ",
            ""
        ],
        "description": "tsconfig"
    },
    "launch config": {
        "prefix": "launchconf",
        "body": [
            "{",
            "    \"version\": \"0.2.0\",",
            "    \"configurations\": [",
            "        {",
            "            \"type\": \"node\",",
            "            \"request\": \"attach\",",
            "            \"name\": \"Attach NodeJS Process\",",
            "            \"address\": \"localhost\",",
            "            \"port\": 9229",
            "        },",
            "        {",
            "            \"name\": \"Debug NodeJS Typescript\",",
            "            \"type\": \"node\",",
            "            \"request\": \"launch\",",
            "            \"sourceMaps\": true,",
            "            \"protocol\": \"inspector\",",
            "            \"internalConsoleOptions\": \"openOnSessionStart\", // move to debug console when tasks done",
            "            \"console\": \"internalConsole\", // use vscode console",
            "            \"runtimeArgs\": [\"--nolazy\", \"-r\", \"ts-node/register\"], // ts-node",
            "            \"cwd\": \"${workspaceRoot}\", // root dir",
            "            \"args\": [\"${workspaceRoot}/src/main.ts\"] // main file",
            "        }",
            "    ]",
            "}",
            ""
        ],
        "description": "launch config"
    }
}
