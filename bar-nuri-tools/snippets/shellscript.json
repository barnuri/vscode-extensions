    "extensions export": {
        "prefix": "extexport",
        "body": [
            "# linux",
            "code --list-extensions | xargs -L 1 echo",
            "# windows",
            "code --list-extensions | foreach-object -process {echo \"code --install-extension $_\"}"
        ],
        "description": "install extensions"
    }
